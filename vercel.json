{
  "rewrites": [
    {
      "source": "/",
      "destination": "/Website/index.html"
    },
    {
      "source": "/games/pong",
      "destination": "/Games/RL-PongGame/frontend/index_local.html"
    }
  ],
  "headers": [
    {
      "source": "/Games/RL-PongGame/frontend/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/Games/RL-PongGame/frontend/pong_agent.onnx",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "cross-origin"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
